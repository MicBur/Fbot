# PostgreSQL Configuration
POSTGRES_USER=youruser
POSTGRES_PASSWORD=yourpassword
POSTGRES_DB=trading_bot_db
POSTGRES_HOST=postgres # Service name in docker-compose

# Redis Configuration
REDIS_HOST=redis # Service name in docker-compose

# Alpaca API Keys
ALPACA_API_KEY=YOUR_ALPACA_API_KEY
ALPACA_SECRET_KEY=YOUR_ALPACA_SECRET_KEY
ALPACA_PAPER=true # or false for live trading

# Financial Modeling Prep API Key
FMP_API_KEY=YOUR_FMP_API_KEY

# Gemini Pro API Key (for Google Deep Research)
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# FastAPI App specific (if any)
# Example:
# APP_SECRET_KEY=a_very_secret_key
# FastAPI base URL (if needed for services to call back)
# FASTAPI_BASE_URL=http://fastapi_app:8000